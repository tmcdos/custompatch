{
  "name": "@smushytaco/custompatch",
  "version": "1.0.0",
  "description": "Tool for patching buggy NPM packages instead of forking them",
  "author": "SmushyTaco",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmushyTaco/custompatch.git"
  },
  "bin": {
    "custompatch": "./dist/src/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "start": "node ./dist/src/cli.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "patching",
    "buggy",
    "packages"
  ],
  "dependencies": {
    "commander": "^12.1.0",
    "diff": "^7.0.0",
    "pacote": "^21.0.0",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/diff": "^6.0.0",
    "@types/node": "^22.10.2",
    "@types/pacote": "^11.1.8",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  }
}
